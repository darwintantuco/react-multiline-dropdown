{
  "name": "react-multiline-dropdown",
  "version": "0.3.0",
  "description": "Custom dropdown with multiline options",
  "main": "build/index.js",
  "scripts": {
    "build": "babel lib -d build",
    "build:watch": "babel lib -w -d build",
    "lint:js": "eslint lib/**",
    "prettier:check": "prettier-eslint --list-different 'lib/**/*.js' 'styles/**/*.scss'",
    "prettier:fix": "prettier-eslint --write 'lib/**/*.js' 'styles/**/*.scss'",
    "lint:watch": "esw -w lib/**",
    "prepublish": "yarn run build"
  },
  "keywords": [
    "react",
    "component",
    "select",
    "dropdown",
    "multiline"
  ],
  "author": "darwin christopher tantuco",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "10.0.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "prettier-eslint-cli": "4.7.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "standard": "12.0.1"
  },
  "dependencies": {
    "styled-components": "^3.2.5"
  }
}
